---
import Layout from "../layouts/Layout.astro";
import { readProductsTransformed, readQuestions } from "../util/provider";
import { Questions } from "../components/Questions";
// export const prerender = true

const questions = await readQuestions();
const products = await readProductsTransformed();

const url = Astro.url;
---

<Layout title="Welcome to Astro.">
	<Questions filters={questions} products={products} initialUrl={url} client:load />
</Layout>
